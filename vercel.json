{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "jarvis-api/api/jarvis.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/jarvis",
      "dest": "jarvis-api/api/jarvis.py"
    }
  ]
}
