{
  "installCommand": "pip install -r jarvis-api/requirements.txt",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/jarvis-api/api/jarvis.py"
    },
    {
      "source": "/health",
      "destination": "/jarvis-api/api/jarvis.py"
    },
    {
      "source": "/pricing",
      "destination": "/jarvis-api/api/jarvis.py"
    }
  ]
}
